{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "trailingSlash": false,
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/feed",
        "destination": "/feed.xml"
      },
      {
        "source": "/sitemap",
        "destination": "/sitemap.xml"
      }
    ],
    "redirects": [
      {
        "source": "/wealthfront",
        "destination": "https://www.wealthfront.com/c/affiliates/invited/AFFB-0QSH-4D1S-PE56",
        "type": 302
      }
    ],
    "headers": [
      {
        "source": "**/*.@(png|jpg|jpeg|webp|gif|svg|ico)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(pdf)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=432000"
          }
        ]
      }
    ]
  }
}
